def func_stock(prices):
    answer = []
    for i in range(0, len(prices)):
        cnt = 0
        for j in range(i+1, len(prices)):
            if prices[i] <= prices[j]:
                cnt += 1
            else:
                cnt += 1
                break
        answer.append(cnt)
    return answer

func_stock([1, 2, 3, 2, 3])
